{
  "name": "Miranda",
  "author": "Thibault Charbonnier <thibaultcha@me.com>",
  "version": "1.0.0",
  "description": "A static website and blog generator",
  "keywords": [
    "static",
    "generator",
    "blog",
    "website"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "miranda": "./bin/miranda"
  },
  "scripts": {
