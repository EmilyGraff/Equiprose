{
  "name": "equiprose",
  "author": "Thibault Charbonnier <thibaultcha@me.com>",
  "version": "1.0.0",
  "description": "A static website and blog generator",
  "keywords": [
    "static",
    "generator",
    "blog",
    "website"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "equiprose": "./bin/equiprose"
  },
  "scripts": {
    "test": "make test",
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/thibaultCha/Equiprose.git"
  },
  "dependencies": {
    "express": "3.4.x",
    "commander": "2.0.x",
    "jade": "0.35.x",
    "stylus": "0.38.x",
    "marked": "0.2.x",
    "moment": "2.4.x",
    "emoji-images": "0.1.x",
    "colors": "0.6.x",
    "yamljs": "0.1.x",
    "fs-extra": "0.8.x",
    "pygmentize-bundled": "2.1.x"
  },
  "devDependencies": {
    "mocha": "1.13.x",
    "blanket": "1.1.x",
    "coveralls": "2.3.x",
    "mocha-lcov-reporter": "0.0.x"
  },
  "engines": {
    "node": ">=0.10.x"
  }
}
